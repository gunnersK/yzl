    <!-- 模糊查询 -->
    <select id="selectByCondition" parameterType="java.lang.String" resultType="com.yzl.pojo.YzlEpc">
    	SELECT * FROM yzl_epc WHERE ename LIKE "%"#{value}"%"
    </select>
    
